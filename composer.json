{
  "name": "moritz-sauer-13/turnstile-spamprotection",
  "description": "A SilverStripe spam protector and form field using Cloudflare's Turnstile implementation",
  "type": "silverstripe-vendormodule",
  "keywords": ["silverstripe", "recaptcha", "turnstile", "cloudflare", "spamprotection"],
  "license": "BSD-3-Clause",
  "authors": [
    {
      "name": "Moritz Sauer",
      "homepage": "https://www.tietge.com",
      "role": "Developer"
    }
  ],
  "require": {
    "silverstripe/framework": "^5",
    "silverstripe/spamprotection": "^4"
  },
  "prefer-stable": true,
  "minimum-stability": "dev",
  "autoload": {
    "psr-4": {
      "UndefinedOffset\\NoCaptcha\\": "src/"
    }
  },
  "extra": {
    "expose": [
      "javascript"
    ]
  }
}